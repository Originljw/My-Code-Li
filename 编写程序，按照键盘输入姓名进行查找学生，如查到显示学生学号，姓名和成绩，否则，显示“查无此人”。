stu_list=[["2021001","小明",['外语','数学','计算机'],[85,78,93]],\
          ["2021002","小强",['外语','数学','计算机'],[68,77,81]],\
          ["2021003","小亮",['外语','数学','计算机'],[91,82,94]],\
          ["2021004","小杰",['外语','数学','计算机'],[70,72,80]]]
s=input("请输入姓名：")
x=False
for i in stu_list:
    if s==i[1]:
        x=True
        print(i[0],i[1],i[2],i[3])
        break
if x==False:
    print("查无此人")
